{
    "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:react-hooks/recommended"],
    "plugins": ["react", "react-hooks"],
    "rules": {
      "no-unused-vars": "warn",
      "import/no-unresolved": "error",
      "import/named": "error",
      "import/default": "error",
      "import/namespace": "error"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
  